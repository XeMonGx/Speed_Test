      <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button layoutX="17.0" layoutY="15.0" mnemonicParsing="false" onAction="#back" text="back" />
         </children>
      </Pane>
   </top>
   <left>
      <Pane prefHeight="320.0" prefWidth="245.0" BorderPane.alignment="CENTER" />
